{
  "manifest_version": 2,
  "name": "Redmine Companion",
  "description": "Redmine Companion",
  "version": "2.0",
  "author": "Güven Şahin",
  "homepage_url": "https://guvensahin.com",
  "key": "",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },

  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage"
  ],

  "background": {
    "scripts": [ "js/jquery-3.6.0.min.js", "js/redmineHelper.js", "js/background.js" ]
  },

  "browser_action": {
    "default_icon": {
      "19": "images/icon48.png",
      "38": "images/icon48.png"
    },
    "default_popup": "html/popup.html"
  },

  "options_page": "html/options.html"
}