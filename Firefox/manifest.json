{
  "manifest_version": 2,
  "name": "Redmine Today's Time Entries",
  "description": "Redmine Today's Time Entries",
  "version": "1.0.1",
  "author": "Semih Ã‡elikol",
  "homepage_url": "http://semihcelikol.com",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },

  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*",
    "storage"
  ],

  "background": {
    "scripts": [ "js/jquery-3.2.1.min.js", "js/redmineHelper.js", "js/background.js" ]
  },

  "browser_action": {
    "default_icon": {
      "19": "img/icon48.png",
      "38": "img/icon48.png"
    },
    "default_popup": "popup.html"
  },

  "options_ui": {
    "page": "options.html"
  }
}